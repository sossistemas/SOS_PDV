  object qrItens: TUniQuery
    Connection = frmModulo.Conexao_Servidor
    SQL.Strings = (
      'select'
      '  ven.data,'
      '  case'
      '    when coalesce(ven.gerou_sat,'#39'N'#39') = '#39'S'#39' then'
      '      '#39'SAT'#39
      '    when coalesce(ven.nfce,'#39'N'#39') = '#39'S'#39' then'
      '      '#39'NFC-e'#39
      '    else'
      '      '#39'Simples'#39
      '  end tipo,'
      '  ven.codproduto,'
      '  pro.produto,'
      '  pro.unidade,'
      '  ven.unitario,'
      '  ven.qtde,'
      '  ven.desconto,'
      '  ven.total,'
      '  ven.codcliente,'
      '  ven.CUPOM_ITEM,'
      '  ven.situacao,'
      '  ven.devolvido'
      'from'
      '  c000032 ven'
      '  inner join c000025 pro on pro.codigo = ven.codproduto'
      'where ven.cupom_numero = :numero')
    Left = 352
    Top = 176
    ParamData = <
      item
        DataType = ftString
        Name = 'numero'
        ParamType = ptInput
        Value = '900363'
      end>
    object qrItensDATA: TDateField
      FieldName = 'DATA'
    end
