  object query: TUniQuery
    Connection = frmModulo.conexao
    SQL.Strings = (
      'SELECT CODIGO'
      '      ,COD_BARRA'
      '      ,NOME'
      '      ,UNIDADE'
      '      ,PRECO_VENDA'
      '      ,NCM'
      '      ,CSOSN'
      '      ,CFOP'
      
        '      ,CASE WHEN CODPRODUTO <> '#39'0'#39' THEN TRUNC(ESTOQUE / QTD_VOLU' +
        'ME)'
      '                                   ELSE ESTOQUE'
      '                                   END ESTOQUE'
      '      ,IDONLINE'
      '      ,EMDESTAQUE'
      'FROM ESTOQUE'
      '')
    Left = 228
    Top = 248
    object queryCODIGO: TIntegerField
      FieldName = 'CODIGO'
      Required = True
